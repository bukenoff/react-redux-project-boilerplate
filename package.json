{
  "name": "react-project-boilerplate",
  "version": "1.0.0",
  "description": "greatest boilerplate you can ever find",
  "main": "src/index.js",
  "private": "true",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collect-coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "make-badges": "istanbul-badges-readme"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && istanbul-badges-readme && git add 'README.md'"
    }
  },
  "repository": {
    "url": "https://github.com/Bukenov-s/react-project-boilerplate"
  },
  "keywords": [],
  "author": "serik",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.0",
    "@sentry/react": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "axios": "0.21.1",
    "connected-react-router": "^6.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "5.10.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^25.1.4",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@vitejs/plugin-react": "^1.0.5",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^5.0.6",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-badges-readme": "^1.2.0",
    "jest": "^25.1.0",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1",
    "sass": "^1.43.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^1.0.1",
    "vite": "^2.6.10"
  }
}
